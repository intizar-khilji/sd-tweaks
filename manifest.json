{
    "manifest_version": 3,
    "name": "SD Tweaks",
    "version": "1.0",
    "permissions": [
        "contextMenus",
        "tabs",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://rajshaladarpan.nic.in/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}